    "ethers": "^5.4.4",
    "hardhat": "^2.6.0",
    "@openzeppelin/contracts": "^4.5.0",
    "axios": "^0.19.0",
    "bn-chai": "^1.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "fixed-merkle-tree": "^0.3.4",
    "commander": "^4.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
